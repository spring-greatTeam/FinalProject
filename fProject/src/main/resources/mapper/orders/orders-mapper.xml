<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- <mapper namespace="member"> -->
<mapper namespace="com.project.fin.orders.model.dao.OrdersDao">
	<resultMap type="orders" id="ordersMap"></resultMap>
	<select id="selectOrdersList" resultMap="ordersMap">
		select ORDER_NO, STORE_NAME, MENU_NAME, PRICE, TOTALPRICE, ORD_ADDRESS from orders where member_id = 'user01'
	</select>
</mapper>








