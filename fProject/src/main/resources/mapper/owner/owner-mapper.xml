<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.project.fin.owner.model.dao.OwnerDao">
	<insert id="insertStore">
		INSERT INTO STORE
		VALUES (
			SEQ_STORE.NEXTVAL,
 			#{CATEGORY},
			#{STORENAME},
			#{ADDRESS},
			#{PHONE},
			#{OPENTIME},
			#{CLOSETIME},
			#{MINPRICE},
			#{DAYOFF},
			#{CONTENT}
		)
	</insert>
</mapper>